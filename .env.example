# Application
APP_NAME=anny
APP_ENV=development
APP_DEBUG=true
APP_HOST=0.0.0.0
APP_PORT=8000

# Logging
LOG_LEVEL=INFO

# Google Service Account
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=path/to/service-account-key.json

# Google Analytics 4
GA4_PROPERTY_ID=properties/123456789

# Google Search Console
SEARCH_CONSOLE_SITE_URL=https://example.com
